{% extends 'base.html.twig' %}

{% block title %}List of Rentals{% endblock %}

{% block body %}
    {% include('partials/admin/navbaradmin.html.twig') %}

    <div class="example-wrapper">
        <h2>List of users</h2>

        <table class="ui celled table">
            <tr>
                <th>ID</th>
                <th>Firstname</th>
                <th>Lastname</th>
                <th>Username</th>
            </tr>
            {% for x in data %}
                <tr>
                    <td>{{ x.id }}</td>
                    <td>{{ x.firstName }}</td>
                    <td>{{ x.lastName }}</td>
                    <td>{{ x.username }}</td>
                </tr>
            {% endfor %}
        </table>

    </div>


{% endblock %}
